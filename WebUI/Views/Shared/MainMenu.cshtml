@model IEnumerable<Domain.EF.Menu>

@{
    var productCategory = (IEnumerable<Domain.EF.ProductCategory>)ViewBag.ProductCategory;
}

<div class="container">
    <div class="head-top">
        <div class="logo">
            <a href="/" ><img src="~/Assets/Client/images/logo.png" alt=""></a>
        </div>
        <div class="h_menu4">
            <ul class="memenu skyblue">
                @foreach (var item in Model) {
                    <li>
                        <a class="color1" href="@item.Link" target="@item.Target">  @item.Text </a>
                    </li>
                }
            </ul>
        </div>

        <div class="clearfix"> </div>
    </div>
</div>